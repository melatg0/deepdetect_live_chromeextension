{
    "manifest_version": 3,
    "name": "Deep Detect Live Extensions",
    "description": "Detects if there are deepfake videos on users website.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.jfif"
    },
    "web_accessible_resources": [{
        "resources": [
            "node_modules/onnxruntime-web/dist/ort.min.js",
            "node_modules/onnxruntime-web/dist/ort-wasm-simd-threaded.wasm",
            "node_modules/onnxruntime-web/dist/ort-wasm-simd-threaded.jsep.wasm",
            "model.onnx",
            "script.js"
        ],
        "matches": ["<all_urls>"]
    }],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
    }],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
}